# Discord Bot Configuration
DISCORD_TOKEN=your_bot_token_here
BOT_PREFIX=!
DATABASE_URL=sqlite:///tickets.db

# Optional: Admin user IDs (comma-separated)
ADMIN_USER_IDS=123456789012345678,987654321098765432

# Optional: Error logging channel ID
ERROR_LOG_CHANNEL_ID=123456789012345678

# Ticketing System Configuration
# Support role for ticket permissions (leave empty to use everyone)
SUPPORT_ROLE_ID=123456789012345678

# Ticket category ID (optional)
TICKET_CATEGORY_ID=123456789012345678

# Auto-close inactive tickets (days)
TICKET_AUTO_CLOSE_DAYS=7

# Admin alert channel for ticket actions
TICKET_ALERT_CHANNEL_ID=123456789012345678